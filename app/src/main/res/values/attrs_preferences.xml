<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DirectoryPickerPreference">
        <attr name="selection" format="enum">
            <enum name="single" value="0"/>
            <enum name="multi" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="BiosDirectoryPickerPreference">
        <attr name="consoleType" format="enum">
            <enum name="ds" value="0"/>
            <enum name="dsi" value="1"/>
        </attr>
    </declare-styleable>
</resources>